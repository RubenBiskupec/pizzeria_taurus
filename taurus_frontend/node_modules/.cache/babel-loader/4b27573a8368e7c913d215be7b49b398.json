      return this.$store.getters.pizzaTypes;\n    },\n    availableBeverages() {\n      return this.$store.getters.availableBeverages;\n    }\n  },\n  mounted() {\n    document.title = \"Menu | Taurus\";\n  }\n}\n</script>\n\n<template>\n  <v-container>\n    <v-navigation-drawer\n      permanent\n      class=\"menu-navigation absolute-left secondary\"\n    >\n      <v-list class=\"pa-4\">\n        <v-list-item-title class=\"text-h3\">\n          Menu - {{ filter }}\n        </v-list-item-title>\n        <v-list-item\n          v-for=\"filter in filters\"\n          v-bind:key=\"filter\"\n        >{{ filter }}</v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n    <v-row\n      class=\"menu-content\"\n      v-show=\"filter=='Pizze'\"\n    >\n      <v-col\n        v-for=\"menuPizza in availableMenuPizzas\"\n        v-bind:key=\"menuPizza.code\"\n        cols=\"12\"\n        xs=\"12\"\n        sm=\"6\"\n        md=\"4\"\n        lg=\"3\"\n        xl=\"2\"\n      >\n        <v-card>\n          <v-card-title>\n            {{ menuPizza.name }}\n          </v-card-title>\n          <v-card-subtitle dense>\n            {{ menuPizza.description }}\n          </v-card-subtitle>\n          <v-card-text>\n            Ingredienti:\n            <span\n              v-for=\"ingredient in menuPizza.ingredients\"\n            >\n              {{ ingredient.name }},\n            </span>\n            <v-spacer></v-spacer>\n            Prezzo: â‚¬{{ menuPizza.price }}\n          </v-card-text>\n          <v-card-text>\n\n          </v-card-text>\n        </v-card>\n        <h3></h3>\n        <h3></h3>\n      </v-col>\n    </v-row>\n\n  </v-container>\n</template>\n\n<style scoped>\n\n.absolute-left {\n  position: absolute;\n  left: 0;\n  top: 0;\n}\n\n.menu-navigation {\n  width: 20% !important;\n}\n\n.menu-content {\n  position: absolute;\n  right: 0;\n  width: 80% !important;\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}